<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignInController">
   <children>
      <Pane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="signInButton" layoutX="226.0" layoutY="251.0" mnemonicParsing="false" onAction="#signIn" text="Log in" />
            <Button fx:id="signUpButton" layoutX="226.0" layoutY="289.0" mnemonicParsing="false" onAction="#goTosignUp" text="Sign up" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="244.0" layoutY="67.0" prefHeight="41.0" prefWidth="113.0" text="UPTSoc" textFill="#353394">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <TextField fx:id="codeField" layoutX="226.0" layoutY="167.0" promptText="Code" />
            <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="208.0" promptText="Password" />
         </children>
      </Pane>
   </children>
</AnchorPane>
