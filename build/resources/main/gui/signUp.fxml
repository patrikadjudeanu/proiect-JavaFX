<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignUpController">
   <children>
      <Pane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" layoutX="158.0" layoutY="120.0" prefHeight="33.0" prefWidth="284.0" text="Enter the code given by your tutor:" />
            <TextField fx:id="codeField" layoutX="166.0" layoutY="172.0" prefHeight="39.0" prefWidth="197.0" />
            <Button fx:id="validateButton" layoutX="380.0" layoutY="179.0" mnemonicParsing="false" onAction="#validate" text="Submit" />
            <Button fx:id="cancelButton" layoutX="503.0" layoutY="307.0" mnemonicParsing="false" onAction="#goToSignIn" text="Cancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
